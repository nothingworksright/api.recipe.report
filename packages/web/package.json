{
  "name": "@recipe-report/web",
  "version": "0.1.0",
  "description": "recipe.report web app",
  "repository": "github:nothingworksright/recipe-report",
  "license": "AGPL-3.0-or-later",
  "engines": {
    "node": "16.14.0"
  },
  "scripts": {
    "updates": "echo \"Checking for available dependency updates\" && ncu -u",
    "prettier": "prettier --write src/**/**.{ts,tsx}",
    "linter": "eslint --ext .js,.jsx,.ts,.tsx src/",
    "prebuild": "rimraf build && less-watch-compiler --config less-builder.config.json",
    "build": "DISABLE_ESLINT_PLUGIN=true react-scripts build",
    "develop": "npm run build && concurrently --kill-others \"less-watch-compiler --config less-watcher.config.json\" \"react-scripts start\"",
    "test": "react-scripts test"
  },
  "author": "jmg1138 https://github.com/jmg1138",
  "main": "app.js",
  "readme": "README.md",
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^12.1.3",
    "@testing-library/user-event": "^13.5.0",
    "@trivago/prettier-plugin-sort-imports": "^3.2.0",
    "concurrently": "^7.0.0",
    "eslint": "8.9.0",
    "eslint-config-prettier": "8.4.0",
    "eslint-plugin-prettier": "4.0.0",
    "npm-check-updates": "12.4.0",
    "prettier": "2.5.1"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.7.2",
    "@types/jest": "^27.4.0",
    "@types/node": "^17.0.18",
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.11",
    "@types/react-helmet": "^6.1.5",
    "@types/react-redux": "^7.1.22",
    "history": "5",
    "jwt-decode": "3.1.2",
    "less": "^4.1.2",
    "less-watch-compiler": "^1.16.3",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-helmet": "^6.1.0",
    "react-hook-form": "7.27.1",
    "react-redux": "7.2.6",
    "react-router-dom": "^6.2.1",
    "react-scripts": "5.0.0",
    "redux": "4.1.2",
    "redux-thunk": "2.4.1",
    "tsconfig-paths": "3.12.0",
    "typescript": "~4.5.5"
  }
}
