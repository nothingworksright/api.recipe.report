    "cors": "2.8.5",
    "express": "4.18.1",
    "helmet": "5.0.2",
    "inversify": "^6.0.1",
    "jwt-simple": "0.5.6",
    "mailer-send-ts": "1.6.0",
    "pg": "8.7.3",
    "reflect-metadata": "^0.1.13",
    "tsc-alias": "1.6.7",
    "tsconfig-paths": "3.14.1",
    "typescript": "4.6.4",
    "uuid": "8.3.2"
  },
  "packageManager": "npm@8.3.1"
}
