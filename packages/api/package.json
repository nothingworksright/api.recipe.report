    "npm-check-updates": "12.5.10",
    "prettier": "2.6.2"
  },
  "dependencies": {
    "@types/cors": "2.8.12",
    "@types/express": "4.17.13",
    "@types/jwt-simple": "0.5.33",
    "@types/node": "17.0.30",
    "@types/pg": "8.6.5",
    "@types/uuid": "8.3.4",
    "@typescript-eslint/eslint-plugin": "5.21.0",
    "@typescript-eslint/parser": "5.21.0",
    "@vvo/tzdb": "^6.50.0",
    "@zxcvbn-ts/core": "2.0.1",
