{
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc", "closure"]
    },
    "source": {
        "include": ["routes/routes.js"]
    },
    "templates": {
        "systemName": "Grocereport",
        "copyright": "nothingworksright.com",
        "includeDate": "true",
        "navType": "inline",
        "theme": "lumen",
        "linenums": "true",
        "collapseSymbols": "false",
        "inverseNav": "true",
        "outputSourceFiles": "true",
        "syntaxTheme": "default",
        "sort": "true",
        "search": "true",
        "default": {
            "includeDate": true
        },
        "cleverLinks": true,
        "monospaceLinks": true
    },
    "opts": {
        "encoding": "utf8",
        "destination": "./docs/",
        "recurse": true
    }
}